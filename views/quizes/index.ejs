<form method="get" action="/quizes">
	Buscar pregunta:
	<input type="text" name="search" />
	<input type="submit" value="Buscar" />
</form>

<table>
<%
	var i;
	for (i=0; i < quizes.length; i++) {
%>
		<tr><td><a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></tr></td>
<%	
	};
%>
</table>

<%
if (search) {
%>
		<p>(Resultados filtrados por "<%= search %>")</p>
<%
	};
%>
